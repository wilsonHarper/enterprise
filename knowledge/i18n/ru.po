"        \"<span style=\"font-weight: bolder;\">?</span>\".\n"
"    </span>"
msgstr ""
"<span style=\"font-size: 14px;\">\n"
"        Хотите работать\n"
"        <span style=\"font-style: italic;\">еще</span>\n"
"        быстрее?\n"
"    </span>\n"
"    <br/>\n"
"    <span style=\"font-size: 14px;\">\n"
"        Зайдите в \n"
"        <span style=\"font-weight: bolder;\">\n"
"            <font class=\"text-o-color-2\">Публикации</font>,\n"
"        </span>\n"
